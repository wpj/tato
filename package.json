{
  "name": "@wpj/tato",
  "description": "Static site generator for an offline recipe browsing app",
  "type": "module",
  "version": "0.0.4",
  "license": "MIT",
  "bin": {
    "tato": "./dist/tato.js"
  },
  "author": "Wyatt Johnston <w@wyatt.page>",
  "homepage": "https://github.com/wpj/tato#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wpj/tato"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "node --loader esbuild-node-loader tools/dev.ts",
    "build": "pnpm run build:tato && pnpm run build:app && pnpm run build:netlify-cms-admin",
    "build:app": "node --loader esbuild-node-loader tools/build.ts",
    "build:content": "node -r source-map-support/register dist/tato.js build --dir ./content",
    "build:netlify-cms-admin": "vite build --config src/admin/vite.config.js",
    "build:tato": "node --loader esbuild-node-loader tools/build-tato.ts",
    "tato": "node dist/tato.js",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "npm run test:jest --",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "classcat": "^4.1.0",
    "csstype": "^2.6.13",
    "fs-extra": "^9.0.1",
    "hasha": "^5.2.2",
    "image-size": "^0.9.2",
    "js-search": "^2.0.0",
    "js-yaml": "^4.1.0",
    "julienne": "^0.4.3",
    "lodash": "^4.17.20",
    "lqip": "^2.1.0",
    "param-case": "^3.0.3",
    "polished": "^3.6.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-frontmatter": "^2.0.0",
    "remark-parse": "^8.0.3",
    "remark-rehype": "^7.0.0",
    "sade": "^1.7.4",
    "sharp": "^0.26.2",
    "totalist": "^2.0.0",
    "typeface-work-sans": "^0.0.72",
    "typography": "^0.16.19",
    "unified": "^9.2.0",
    "unist-builder": "^2.0.3",
    "unist-util-visit": "^2.0.3",
    "workbox-build": "^6.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@julienne/static": "^0.1.0",
    "@svgr/cli": "^5.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/express": "^4.17.13",
    "@types/hast": "^2.3.1",
    "@types/jest": "^26.0.24",
    "@types/js-search": "^1.4.0",
    "@types/js-yaml": "^4.0.2",
    "@types/mdast": "^3.0.3",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.11.10",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.0",
    "@types/sade": "^1.7.2",
    "@types/sharp": "^0.26.0",
    "@types/typography": "^0.16.3",
    "@types/unist": "^2.0.3",
    "@vanilla-extract/babel-plugin": "^1.0.1",
    "@vanilla-extract/css": "^1.2.1",
    "@vanilla-extract/vite-plugin": "^1.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-jest": "^27.0.6",
    "cross-env": "^7.0.2",
    "esbuild": "^0.12.17",
    "esbuild-node-loader": "^0.1.1",
    "express": "^4.17.1",
    "feather-icons": "^4.28.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^10.4.2",
    "netlify-cms-app": "^2.15.33",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "source-map-support": "^0.5.19",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "volta": {
    "node": "14.17.4",
    "npm": "7.20.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
