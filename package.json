  "scripts": {
    "build": "npm run build:site && npm run build:cli",
    "build:site": "BABEL_ENV=site babel --extensions '.ts,.tsx' src/site --out-dir dist --ignore '**/*.d.ts' --copy-files",
    "build:cli": "BABEL_ENV=cli microbundle --entry src/tato.ts --format cjs --target node",
    "tato": "node dist/tato.js",
    "typecheck": "tsc --noEmit"
  },
